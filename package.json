    "build:docs": "typedoc guest-js/index.ts",
    "format": "prettier -w ."
  },
  "dependencies": {
    "@tauri-apps/api": ">=2.0.0-beta.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "prettier": "^3.3.2",
    "rollup": "^4.9.6",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.13",
    "typescript": "^5.3.3"
  }
